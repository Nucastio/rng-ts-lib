{
  "name": "rng-ts-lib",
  "version": "1.2.2",
  "description": "RNG Library",
  "author": "Nucast",
  "homepage": "https://github.com/Nucastio/rng-ts-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nucastio/rng-ts-lib"
  },
  "license": "ISC",
  "keywords": [
    "typescript",
    "npm",
    "package"
  ],
  "main": "dist/cjs/rng-lib.js",
  "module": "dist/esm/rng-lib.js",
  "browser": "dist/browser/rng-lib.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "tsc -p ./tsconfig.json --outDir dist --declaration true && api-extractor run && rollup -c",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir dist --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "eslint": "^8.8.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
